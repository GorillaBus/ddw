{
	"plugins": [],
	"recurseDepth": 10,
	"opts": {
		"encoding": "utf8",
		"destination": "./doc/",
		"recurse": true
	},
	"source": {
		"include": ["src/"],
		"exclude": ["node_modules/"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}
